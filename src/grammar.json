{
  "name": "todolang",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "todo_definition"
        }
      ]
    },
    "todo_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "todo"
        },
        {
          "type": "PATTERN",
          "value": "\\s+"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        }
      ]
    },
    "text": {
      "type": "PATTERN",
      "value": ".*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
